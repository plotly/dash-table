{
    "extends": "./tsconfig.base.json",
    "compilerOptions": {
        "outDir": "./dist",
        "baseUrl": ".",
        "paths": {
            "core/*": [
                "src/core/*"
            ],
            "dash-table/*": [
                "src/dash-table/*"
            ],
            "tests": [
                "src/tests"
            ]
        }
    },
    "exclude": [
        "dash_table/*",
        "dist/*",
        "node_modules/*",
        "storybook-static/*",
        "tests/*",
        "venv/*",
        "vv/*"
    ]
}