{
    "extends": "./tsconfig.base.json",
    "compilerOptions": {
        "outDir": "./dist",
        "baseUrl": ".",
        "paths": {
            "core/*": [
                "src/core/*"
            ],
            "dash-table/*": [
                "src/dash-table/*"
            ],
            "tests/*": [
                "tests/*"
            ]
        }
    },
    "include": [
        "src/**/*",
        "demo/**/*",
        "tests/**/*",
        "@Types/**/*"
    ]
}